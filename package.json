{
  "name": "lol-skin-viewer",
  "version": "0.1.0",
  "description": "League of Legends Owned Skin Viewer using LCU API + Data Dragon",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder --win --x64"
  },
  "build": {
    "appId": "com.arjun.lolskinviewer",
    "productName": "LoL Skin Viewer",
    "win": {
      "target": "portable",
      "icon": "assets/icon.ico"
    },
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "electron": "^29.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  }
}